msgstr "продаёт {} за {}"

#: src/handlers/__init__.py:228
msgid "buys {} for {}"
msgstr "покупает {} за {}"

#: src/handlers/__init__.py:282
msgid "Finish"
msgstr "Завершить"

#: src/handlers/__init__.py:295
msgid "Similar"
msgstr "Похожие"

#: src/handlers/__init__.py:298
msgid "Match"
msgstr "Подобрать"

#: src/handlers/__init__.py:304
msgid "Edit"
msgstr "Редактировать"

#: src/handlers/__init__.py:310
