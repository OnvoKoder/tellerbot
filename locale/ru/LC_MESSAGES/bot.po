#: src/handlers/__init__.py:82
msgid "Send positive number."
msgstr "Отправьте положительное число."

#: src/handlers/__init__.py:86
msgid "Send number greater than"
msgstr "Отправьте число большее, чем"

#: src/handlers/__init__.py:112
msgid "There are no orders."
msgstr "Нет заказов."

#: src/handlers/__init__.py:157 src/handlers/__init__.py:261
#: src/handlers/creation.py:156 src/handlers/creation.py:181
